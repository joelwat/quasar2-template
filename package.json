{
    "name": "app-name",
    "version": "0.0.1",
    "description": "App description",
    "productName": "Product name",
    "author": "Joel Watson <joelwat@gmail.com>",
    "private": true,
    "scripts": {
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
        "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --fix",
        "lint:watch": "chokidar '**/*.{(c|m)?js,vue,json}' '!(node_modules/*|dist/*coverage/*)' --cache --initial --debounce -c 'eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore'",
        "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
        "dev": "quasar dev",
        "build": "quasar build",
        "test:unit:ui": "vitest --ui",
        "test:unit": "vitest",
        "test:unit:ci": "vitest run",
        "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://127.0.0.1:9000 \"cypress open --e2e\"",
        "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://127.0.0.1:9000 \"cypress run --e2e\"",
        "test:component": "cross-env NODE_ENV=test cypress open --component",
        "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
    },
    "dependencies": {
        "@quasar/cli": "^2.2.1",
        "@quasar/extras": "^1.16.4",
        "@vue/tsconfig": "^0.4.0",
        "@vueuse/core": "^10.2.0",
        "axios": "^1.4.0",
        "element-plus": "^2.3.7",
        "pinia": "^2.1.4",
        "quasar": "^2.12.0",
        "vue": "^3.3.4",
        "vue-gtag": "^2.0.1",
        "vue-i18n": "^9.2.2",
        "vue-router": "^4.2.2"
    },
    "devDependencies": {
        "@iconify-json/ep": "^1.1.11",
        "@intlify/unplugin-vue-i18n": "^0.11.0",
        "@quasar/app-vite": "^1.4.3",
        "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.1.0",
        "@quasar/quasar-app-extension-testing-unit-vitest": "^0.3.0",
        "@tsconfig/node18": "^2.0.1",
        "@types/cypress__code-coverage": "^3.10.0",
        "@types/node": "18.16.18",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "@typescript-eslint/parser": "^5.60.1",
        "@unocss/preset-icons": "^0.53.4",
        "@vitest/ui": "^0.32.2",
        "@vue/eslint-config-airbnb-with-typescript": "^7.0.0",
        "@vue/test-utils": "^2.4.0",
        "autoprefixer": "^10.4.14",
        "chokidar-cli": "^3.0.0",
        "cross-env": "^7.0.3",
        "cypress": "^12.16.0",
        "dotenv": "^16.3.1",
        "eslint": "^8.43.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-cypress": "^2.13.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-vue": "^9.15.1",
        "start-server-and-test": "^2.0.0",
        "tslib": "^2.6.0",
        "typescript": "5.1.5",
        "unocss": "^0.53.4",
        "vite-plugin-inspect": "^0.7.29",
        "vite-tsconfig-paths": "^4.2.0",
        "vitest": "^0.32.2"
    },
    "engines": {
        "node": "^18",
        "yarn": ">= 1.22.19"
    },
    "packageManager": "yarn@3.6.0"
}
