{
    "name": "quasar2-template",
    "version": "0.0.1",
    "description": "Quasar v2 Template",
    "productName": "Quasar v2 Template",
    "author": "Joel Watson <joelwat@gmail.com>",
    "type": "module",
    "scripts": {
        "build": "concurrently npm:typecheck npm:build-only",
        "build-only": "quasar build",
        "dev": "quasar dev",
        "lint": "eslint . --cache --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
        "lint:fix": "eslint . --cache --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --fix",
        "lint:watch": "chokidar '**/*.{(c|m)?js,vue,json}' '!(node_modules/*|dist/*coverage/*)' --cache --initial --debounce -c 'eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore'",
        "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
        "test:component": "cross-env NODE_ENV=test cypress open --component -p3000",
        "test:component:ci": "cross-env NODE_ENV=test cypress run --component -p3000 --browser chrome",
        "test:e2e": "cross-env NODE_ENV=test start-test \"yarn dev -p5173\" https-get://localhost:5173 \"cypress open --e2e\"",
        "test:e2e:ci": "cross-env NODE_ENV=test start-test \"yarn dev -p5173\" http-get://127.0.0.1:5173 \"cypress run --e2e\"",
        "test:unit": "vitest",
        "test:unit:ci": "vitest run",
        "test:unit:ui": "vitest --ui",
        "test:unit:watch": "vitest watch",
        "typecheck": "vue-tsc --noEmit -p tsconfig.app.json --composite false"
    },
    "dependencies": {
        "@quasar/extras": "^1.16.11",
        "@vueuse/core": "^10.10.0",
        "axios": "^1.7.2",
        "pinia": "^2.1.7",
        "quasar": "^2.16.4",
        "vue": "^3.4.27",
        "vue-i18n": "^9.13.1",
        "vue-router": "^4.3.3",
        "yaml": "^2.4.5"
    },
    "devDependencies": {
        "@iconify-json/fa6-solid": "^1.1.21",
        "@intlify/eslint-plugin-vue-i18n": "^3.0.0",
        "@intlify/unplugin-vue-i18n": "^4.0.0",
        "@quasar/app-vite": "2.0.0-beta.14",
        "@quasar/quasar-app-extension-testing-e2e-cypress": "^6.1.0",
        "@quasar/quasar-app-extension-testing-unit-vitest": "^1.0.0",
        "@quasar/vite-plugin": "^1.7.0",
        "@rollup/plugin-yaml": "^4.1.2",
        "@tsconfig/node20": "^20.1.4",
        "@types/cypress__code-coverage": "^3.10.2",
        "@types/node": "20.14.2",
        "@typescript-eslint/eslint-plugin": "^7.13.0",
        "@typescript-eslint/parser": "^7.13.0",
        "@unocss/preset-icons": "^0.61.0",
        "@vitejs/plugin-vue": "^5.0.5",
        "@vue/test-utils": "^2.4.6",
        "autoprefixer": "^10.4.19",
        "chokidar-cli": "^3.0.0",
        "concurrently": "^8.2.2",
        "cross-env": "^7.0.3",
        "cypress": "^13.11.0",
        "eslint": "^8.57.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^18.0.0",
        "eslint-plugin-cypress": "^3.3.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.34.2",
        "eslint-plugin-vue": "^9.26.0",
        "eslint-plugin-vuejs-accessibility": "^2.3.0",
        "happy-dom": "^14.12.0",
        "meow": "^13.2.0",
        "optionator": "^0.9.4",
        "postcss": "^8.4.38",
        "start-server-and-test": "^2.0.4",
        "stylelint": "^16.6.1",
        "ts-node": "^10.9.2",
        "tslib": "^2.6.3",
        "typescript": "^5.4.5",
        "unocss": "^0.61.0",
        "unplugin-vue-router": "^0.9.1",
        "vite": "^5.2.13",
        "vite-plugin-checker": "^0.6.4",
        "vite-plugin-inspect": "^0.8.4",
        "vite-plugin-vue-layouts": "^0.11.0",
        "vite-tsconfig-paths": "^4.3.2",
        "vitest": "^1.6.0",
        "vue-eslint-parser": "^9.4.3",
        "vue-tsc": "1"
    },
    "engines": {
        "node": ">=20.9",
        "npm": ">= 6.13.4",
        "yarn": ">= 1.21.1"
    },
    "packageManager": "yarn@4.3.0"
}
